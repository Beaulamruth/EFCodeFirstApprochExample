@model List<EFCodeFirstApprochExample.Identity.ApplicationUser>
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/AdminLayoutPage.cshtml";
}

<h2>Users</h2>
<table class="table table-bordered">
    <tr>
        <th>User Name</th>
        <th>Email</th>
        <th>Mobile</th>
        <th>City</th>
        <th>Country</th>
        <th>Date of Birth</th>
    </tr>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.UserName</td>
            <td>@user.Email</td>
            <td>@user.PhoneNumber</td>
            <td>@user.City</td>
            <td>@user.Country</td>
            <td>@user.Birthday</td>
        </tr>
    }
</table>

